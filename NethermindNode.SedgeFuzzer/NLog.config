<?xml version="1.0" encoding="utf-8" ?>
<nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://www.nlog-project.org/schemas/NLog.xsd NLog.xsd"
      autoReload="true"
      throwExceptions="false">

  <targets>
	  <target name="logfileUbuntu" xsi:type="File" fileName="/root/logs.txt" />
	  <target name="logfileMainDir" xsi:type="File" fileName="logs.txt" />
  </targets>

  <rules>
	  <logger name="*" minlevel="Debug" writeTo="logfileUbuntu" />
	  <logger name="*" minlevel="Debug" writeTo="logfileMainDir" />
  </rules>
</nlog>